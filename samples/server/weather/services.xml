<service name="weather">
    <parameter name="ServiceClass" locked="xsd:false">weather</parameter>
   <description>
       This is a sample eventing data source which can be started and stopped remotely by making
       soap calles to it's start and stop functions respectively.
   </description>
   <module ref="savan"/>
   <parameter name="TopicURL" locked="xsd:false">http://localhost:9090/axis2/services/weather</parameter>

   <operation name="start">
           <parameter name="wsamapping" >http://ws.apache.org/axis2/c/savan/samples/weather/start</parameter>
   </operation>
   <operation name="stop">
           <parameter name="wsamapping" >http://ws.apache.org/axis2/c/savan/samples/weather/stop</parameter>
   </operation>
</service>
